{
    "name": "superbig/craft-usergroupfield",
    "description": "Field type that let you select one or more user groups",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "user group field"
    ],
    "support": {
        "docs": "https://github.com/superbigco/craft-usergroupfield/blob/master/README.md",
        "issues": "https://github.com/superbigco/craft-usergroupfield/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Superbig",
            "homepage": "https://superbig.co"
        }
    ],
    "require": {
        "craftcms/cms": "3.7.45"
    },
    "autoload": {
        "psr-4": {
          "superbig\\usergroupfield\\": "src/"
        }
    },
    "extra": {
        "name": "User Group Field",
        "handle": "user-group-field",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/superbigco/craft-usergroupfield/master/CHANGELOG.md",
        "class": "superbig\\usergroupfield\\UserGroupField"
    }
}
